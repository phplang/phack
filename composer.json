{
  "name": "phplang/phack",
  "description": "HackLang parser for PHP",
  "type": "library",
  "keywords": [
    "hacklang",
    "parser"
  ],
  "homepage": "https://github.com/phplang/phack",
  "license": "BSD",
  "authors": [
    {
      "name": "Sara Golemon",
      "email": "pollita@php.net",
      "homepage": "https://twitter.com/SaraMG",
      "role": "Developer"
    }
  ],
  "require": {
    "nikic/PHP-Parser": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "PhpLang\\Phack\\": "lib/"
    },
    "files": [
      "lib/autoload.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "vimeo/psalm": "dev-phpparser-4"
  },
  "autoload-dev": {
    "psr-4": {
      "PhpLang\\Phack\\Test\\": "tests/"
    }
  },
  "bin": [
    "phack"
  ]
}
